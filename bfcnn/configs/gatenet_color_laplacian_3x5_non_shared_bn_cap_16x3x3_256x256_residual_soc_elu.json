{
  "model_denoise": {
    "filters": 16,
    "no_layers": 5,
    "min_value": 0,
    "add_var": false,
    "max_value": 255,
    "kernel_size": 5,
    "type": "resnet",
    "batchnorm": true,
    "activation": "elu",
    "add_gates": true,
    "clip_values": true,
    "shared_model": false,
    "add_final_bn": false,
    "output_multiplier": 1.0,
    "local_normalization": -1,
    "final_activation": "tanh",
    "add_skip_with_input": false,
    "add_residual_between_models": true,
    "input_shape": ["?", "?", 3],
    "kernel_initializer": "glorot_normal",
    "kernel_regularizer": [{
      "type": "soft_orthogonal",
      "config": {
        "lambda_coefficient": 1.0,
        "l1_coefficient": 0.001
      }
    }],
    "pyramid": {
      "levels": 3,
      "type": "laplacian",
      "xy_max": [2.0, 2.0],
      "kernel_size": [5, 5]
    }
  },
   "train": {
    "epochs": 50,
    "total_steps": -1,
    "checkpoints_to_keep": 3,
    "checkpoint_every": 10000,
    "visualization_number": 4,
    "visualization_every": 1000,
    "random_batch_iterations": 5,
    "random_batch_size": [256, 256, 3],
    "random_batch_min_difference": 0.1,
    "optimizer": {
      "gradient_clipping_by_norm": 1.0,
      "schedule": {
        "type": "exponential_decay",
        "config": {
          "decay_rate": 0.9,
          "decay_steps": 20000,
          "learning_rate": 0.001
        }
      }
    },
    "prune": {
      "start_epoch": 0,
      "strategies": [{
        "type": "minimum_threshold_shrinkage",
        "config": {
          "shrinkage": 0.75,
          "minimum_threshold":   0.00005,
          "shrinkage_threshold": 0.0001
        }
      }]
    }
  },
  "loss": {
    "hinge": 2.5,
    "cutoff": 200.0,
    "mae_multiplier": 1.0,
    "regularization": 0.01,
    "nae_multiplier": 10.0
  },
  "dataset": {
    "batch_size": 16,
    "min_value": 0,
    "max_value": 255,
    "clip_value": true,
    "random_blur": true,
    "subsample_size": -1,
    "round_values": true,
    "random_invert": false,
    "random_rotate": 0.314,
    "random_up_down": true,
    "color_mode": "rgb",
    "random_left_right": true,
    "input_shape": [256, 256, 3],
    "multiplicative_noise": [0.05, 0.1, 0.2],
    "additional_noise": [1, 5, 10, 20, 40, 60, 80],
    "inputs": [
      {
        "dataset_shape": [256, 768],
        "directory": "/media/data1_4tb/datasets/KITTI/data/depth/raw_image_values/"
      }
    ]
  }
}

